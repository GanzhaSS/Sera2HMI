    unit.Global_Property.ProjectFolderAlarms = unit.Global_Property.ProjectFolder + "\\AlarmSettings";
    unit.Global_Property.ProjectFolderTrends = FileSystem1.CombinePath(unit.Global_Property.ProjectFolder,"TrendSettings");     
}
]]>
