		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="MessageReceived">
			<body kind="om">
				<![CDATA[if (messageType == "TitleClose")
   Close();    // Закрытие текущего окна-заголовка

if (messageType == "Title"+String.ToString(MonitorNum)+"setWindowCurr")
   if (WindowCurr != String.ToUint2(messageData,9999)) {
      // Запрос на закрытие окна с номером WindowCurr на текущем мониторе
      unit.Global_WinCtrl.WindowGroupIK.SendToAll("Mon"+String.ToString(MonitorNum)+"WinClose",
                                                  String.ToString(WindowCurr));
      // Установка нового значение для номера открытого окна на текущем мониторе
      WindowCurr = String.ToUint2(messageData,9999);
   }
]]>
			</body>
		</do-on>
		<init target="Group" ver="5" ref="unit.Global_WinCtrl.WindowGroupTitle"/>
	</object>
